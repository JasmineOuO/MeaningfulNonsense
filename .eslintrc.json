{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "strict": 0,
        "react/prop-types": 0,
        "react/forbid-prop-types": 0,
        "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0,
        "import/named": 0,
        "import/no-unresolved": 0,
        "import/extensions": 0
    }
}